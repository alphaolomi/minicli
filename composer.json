{
  "name": "alphaolomi/minicli",
  "description": "Minicli Application Template powered with Termwind and Plates",
  "license": "MIT",
  "type": "project",
  "homepage": "https://alphaolomi.github.io/minicli/",
  "keywords": ["cli","command-line", "template", "termwind", "plates", "minicli"],
  "authors": [
    {
        "name": "Alpha Olomi",
        "email": "hello@alphaolomi.com",
        "role": "Developer"
    }
],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "bin" : ["minicli"],
  "require": {
    "php": "^8.1",
    "minicli/minicli": "^3.0",
    "minicli/command-help": "^0.1.0",
    "nunomaduro/termwind": "^1.15",
    "league/plates": "^3.5"
  },
  "require-dev": {
    "pestphp/pest": "^1.21",
    "laravel/pint": "^1.10"
  },
  "scripts": {
    "test" : ["pest"],
    "lint" : ["pint"]
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
